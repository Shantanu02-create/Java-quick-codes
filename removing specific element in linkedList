import java.util.*;
public class Namelist {
    public static void main(String[] args) {
        
        LinkedList<String> names = new LinkedList<>();

        // Add names to the list
        names.add("Alice");
        names.add("Bob");
        names.add("Charlie");
        names.add("David");
        names.add("Eva");

        System.out.println("Original list: " + names);

        names.remove(2);
        names.remove(3);

        System.out.println("After removing 2nd and 3rd names: " +names);}
}
